o <= 1) {\r\n\r\n        grid.gutters.type = \"vertical\";\r\n\r\n        const dualWidth = layerContainerSize.width * 2;\r\n        const remainingWidth = window.innerWidth - dualWidth;\r\n\r\n        if (isTwoContainers && window.innerWidth > dualWidth) {\r\n            grid.colNum = 2;\r\n            if (gapRatio > 1) {\r\n                grid.gap = remainingWidth / gapRatio;\r\n                grid.gutters.side = (remainingWidth - grid.gap) / 2;\r\n            }\r\n            else {\r\n                grid.gap = remainingWidth;\r\n            }\r\n        }\r\n        else {\r\n            grid.gutters.side = remainingWidth / 2;\r\n        }\r\n    }\r\n    else {\r\n\r\n        grid.gutters.type = \"horizontal\";\r\n\r\n        const dualHeight = layerContainerSize.height * 2;\r\n        const remainingHeight = window.innerHeight - dualHeight;\r\n\r\n        if (isTwoContainers && window.innerHeight > dualHeight) {\r\n            grid.rowNum = 2;\r\n            if (gapRatio > 1) {\r\n                grid.gap = remainingHeight / gapRatio;\r\n                grid.gutters.side = (remainingHeight - grid.gap) / 2;\r\n            }\r\n            else {\r\n                grid.gap = remainingHeight;\r\n            }\r\n        }\r\n        else {\r\n            grid.gutters.side = remainingHeight / 2;\r\n        }\r\n    }\r\n\r\n    return grid;\r\n}","C:\\Users\\DoubleD\\Desktop\\Code\\2020\\017 Uniform Design Test Project\\src\\App\\Responsive\\UniformResponse\\ContainerGrid.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\017 Uniform Design Test Project\\src\\App\\Responsive\\UniformResponse\\LayerContainer.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\017 Uniform Design Test Project\\src\\App\\Responsive\\WindowResize.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\017 Uniform Design Test Project\\src\\App\\settings.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\017 Uniform Design Test Project\\src\\App\\Content\\Layers\\LayerTemplate\\Layer.js",[],"C:\\U