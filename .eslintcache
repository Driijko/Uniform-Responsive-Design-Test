FocusableElements = 5;\r\n\r\n// MAIN COMPONENT ///////////////////////////////////////////////////\r\nexport default function Layer({width, height, phase, children}) {\r\n\r\n    // STATE ///////////////////////////////////////////////////////\r\n    \r\n    // Focus -------------------------------------------------------\r\n    const [tabIndex, setTabIndex] = useState(0);\r\n\r\n    // 'useKey' returns true if key is held down.\r\n    const tab = useKey(\"Tab\");\r\n\r\n    useEffect(()=> {\r\n        if (tab) {\r\n            if (tabIndex === maxFocusableElements) {\r\n                setTabIndex(1);\r\n            }\r\n            else {\r\n                setTabIndex(tabIndex + 1);\r\n            }\r\n        }\r\n    },[tab]);\r\n\r\n    // RENDER ///////////////////////////////////////////////////////\r\n    return (\r\n        <LayerDiv width={width} height={height}>          \r\n            <header>\r\n                <StyledH1 spatial={[0, 2, 20, 3]} width={width}>\r\n                    Modern Art\r\n                </StyledH1>\r\n                <nav>\r\n                    <ul>\r\n                        <li>\r\n                            <NavLink \r\n                                spatial={[0, 9, 20, 3]}     \r\n                                width={width}\r\n                                focus={tabIndex === 1}\r\n                            >\r\n                                Home\r\n                            </NavLink>\r\n                        </li>\r\n                        <li>\r\n                            <NavLink \r\n                                spatial={[0, 12, 20, 3]} \r\n                                width={width}\r\n                                focus={tabIndex === 2}\r\n                            >\r\n                                Articles\r\n                            </NavLink>\r\n                        </li>\r\n                            <NavLink \r\n                                spatial={[0, 15, 20, 3]} \r\n  